include "cores/select.cal"
include "std/io.cal"

# will execute on RM86 backend
version RM86
	"Hello, RM86!\n" printstr
end

# won't execute unless you set the Foo version
version Foo
	"Hello, Foo!\n" printstr
end

# will execute because Foo isn't set
version not Foo
	"Hello, not Foo!\n" printstr
end
